@article{taylor_impact_2003,
  title    = {The impact of issue voting on a local nonpartisan election},
  volume   = {35},
  doi      = {10.1177/0160323x0303500303},
  abstract = {THE RESEARCH PRESENTED in this article examines the role that two particular issues played in a nonpartisan race for mayor in 2000 in Fayetteville, Arkansas, featuring two candidates in a highly contested race. In that race, the two candidatesâ positions were portrayed very differently on the broad issues of environmental protection and economic development, and for the latter part of 2000, the local political landscape was dominated by a controversial city council vote to allow the removal of a large stand of old trees. By election day in November 2000, the seemingly competing goals of economic development and environmental protection embodied in the tree controversy became the centerpiece of the mayoral race. The race in 2000 was nonpartisan; both candidates were registered Democrats, and in a previous mayoral race, the two candidates were separated by only a few hundred votes. The shared partisan affi liations between the candidates, the familiarity of the candidates to the voters, and their parity in the previous mayoral election allow us to isolate the impact of candidatesâ stances on an issue (namely, the tree ordinance controversy) on the outcome of the election. Despite the fact that roughly three-quarters of all local elections in the United States are nonpartisan (Renner and DeSantis 1993), little is known about the dynamics of vote choice in nonpartisan elections compared with partisan elections. Moreover, very little research in this area has featured direct surveys of voters; most of the existing studies of nonpartisan elections have relied on aggregate voting data (Schaffner, Wright, and Streb 2001, 11). In this study, we employed data collected from exit polls in Fayetteville, Arkansas, on election day 2000, which allowed us to gauge the effect of the tree ordinance controversy on votersâ choice for mayor.},
  number   = {3},
  journal  = {State and Local Government Review},
  author   = {Taylor, Jon and Schreckhise, William D.},
  month    = sep,
  year     = {2003},
  pages    = {174--182}
}

