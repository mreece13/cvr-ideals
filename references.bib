@article{de_benedictis-kessner_how_2023,
  title    = {How partisanship in cities influences housing policy},
  issn     = {1556-5068},
  doi      = {10.2139/ssrn.3981805},
  abstract = {Housing policy is one of the most important areas of local politics. Yet little is known about how local legislatures and executives make housing policy decisions and how their elections shape policy in this important realm. We leverage housing policy data and a new data source of 15,573 city council elections and 3,248 mayoral elections in large cities in the United States and a regression discontinuity design to examine partisan divides in housing policy among the mass public as well as the impact of local leadersâ partisanship on housing policy. We provide robust evidence that electing mayors from diï¬erent political parties shapes citiesâ housing stock. Electing a Democrat as mayor leads to increased multifamily housing production. These eï¬ects are concentrated in cities where councils have less power over land use changes. Overall, our paper shows that politics inï¬uences local housing policy, and it contributes to a larger literature on local political economy.},
